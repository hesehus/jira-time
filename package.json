{
    "name": "jira-time",
    "version": "1.1.1",
    "description": "Log JIRA time awesomly!",
    "main": "index.js",
    "engines": {
        "node": ">=4.5.0",
        "npm": "^3.0.0"
    },
    "scripts": {
        "clean": "rimraf dist",
        "compile": "better-npm-run compile",
        "lint": "eslint bin build config server src",
        "lint:fix": "npm run lint -- --fix",
        "start": "better-npm-run start",
        "start:dummyapi": "better-npm-run start:dummyapi",
        "start:prod": "better-npm-run start:prod",
        "dev": "better-npm-run dev",
        "test": "better-npm-run test",
        "test:dev": "npm run test -- --watch",
        "deploy": "better-npm-run deploy",
        "deploy:dev": "better-npm-run deploy:dev",
        "deploy:prod": "better-npm-run deploy:prod",
        "codecov": "cat coverage/*/lcov.info | codecov",
        "nativefier": "nativefier -i src/assets/logo.png --electron-version 6.0.1 --internal-urls /no-urls-opens-in-jira-time/g --single-instance https://prod-jira-time.hesehus.dk"
    },
    "betterScripts": {
        "compile": {
            "command": "node bin/compile",
            "env": {
                "DEBUG": "app:*"
            }
        },
        "dev": {
            "command": "nodemon bin/server --ignore dist --ignore coverage --ignore tests --ignore src",
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "app:*"
            }
        },
        "deploy": {
            "command": "npm run clean && npm run compile",
            "env": {
                "DEBUG": "app:*"
            }
        },
        "deploy:dev": {
            "command": "npm run deploy",
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "app:*"
            }
        },
        "deploy:prod": {
            "command": "npm run deploy",
            "env": {
                "NODE_ENV": "production",
                "DEBUG": "app:*"
            }
        },
        "start": {
            "command": "node bin/server",
            "env": {
                "DEBUG": "app:*"
            }
        },
        "start:prod": {
            "command": "node bin/server",
            "env": {
                "NODE_ENV": "production",
                "DEBUG": "app:*"
            }
        },
        "start:dummyapi": {
            "command": "node --inspect bin/server",
            "env": {
                "DUMMY_API": "1",
                "DEBUG": "app:*"
            }
        },
        "test": {
            "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
            "env": {
                "NODE_ENV": "test",
                "DEBUG": "app:*"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
    },
    "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
    "license": "MIT",
    "dependencies": {
        "babel-loader": "^6.2.5",
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-polyfill": "^6.16.0",
        "babel-preset-es2015": "^6.14.0",
        "babel-preset-react": "^6.11.1",
        "babel-preset-stage-0": "^6.3.13",
        "babel-runtime": "^6.11.6",
        "better-npm-run": "0.0.11",
        "bootstrap-sass": "^3.3.7",
        "css-loader": "^0.25.0",
        "cssnano": "^3.7.4",
        "cuid": "^1.3.8",
        "debug": "^2.2.0",
        "deep-assign": "^2.0.0",
        "dom-closest": "^0.2.0",
        "event-class-es6": "^1.0.3",
        "event-emitter": "^0.3.4",
        "extract-text-webpack-plugin": "^1.0.0",
        "file-loader": "^0.9.0",
        "flatpickr": "^2.4.7",
        "fs-extra": "^0.30.0",
        "hammerjs": "^2.0.8",
        "html-webpack-plugin": "^2.22.0",
        "http": "0.0.0",
        "http-proxy-middleware": "^0.17.2",
        "imports-loader": "^0.6.5",
        "ip": "^1.1.2",
        "json-loader": "^0.5.4",
        "keycode": "^2.1.8",
        "lorem-ipsum": "^1.0.3",
        "moment": "^2.15.2",
        "node-sass": "~4.13.0",
        "normalize.css": "^4.1.1",
        "postcss-loader": "^0.13.0",
        "prop-types": "~15.7.2",
        "react": "~16.11.0",
        "react-dom": "~16.11.0",
        "react-motion": "~0.5.2",
        "react-redux": "~5.0.3",
        "react-router": "~5.1.2",
